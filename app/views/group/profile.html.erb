<div class="intro">
<div class="intro-body">

<h1 class="brand-heading"><%= @group.group_name %></h1>
<h3><%= link_to "Back to your groups", group_index_path %></h3><br><br>
<% @current_group_users.each do |user| %>
	<%= link_to (image_tag user.image_aws,  class: "img-circle"), user  %>
	<h4>Win : <%= user.win %>  Lose: <%= user.win %> </h4>
<% end %>
<h4><%= link_to "Create Match", new_match_path(:id => @group.id), class: "btn btn-primary" %><br>
</h4>

</div>
</div>

