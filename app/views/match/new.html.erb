<div class="intro">
<div class="intro-body">
<h1>Create New Match</h1>
<%= form_for(:matches) do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :match_name %><br>
    <br>
    <%= f.label :group_members %><br>
    <%= f.text_area :members %><br><br>
    <%= f.hidden_field :group_id, :value => @current_group.id %>
    <%= f.submit  "Create Match" %>
  <% end %>
  </div>
  </div>
